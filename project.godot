config_version=4

[application]

config/name="Project R"
config/description="A top down RPG similar to games like Stardew Valley and Harvest Moon based around an animal and exploration."
run/main_scene="res://SceneManager.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="/CoolMoon Studios/ProjectR/"
boot_splash/image="res://Textures/HUD/splash.png"
boot_splash/use_filter=false
boot_splash/bg_color=Color( 0.227451, 0.227451, 0.227451, 1 )
config/icon="res://icon.png"

[autoload]

Global="*res://Resources/Overworld/Global.gd"
PlayerInventory="*res://Resources/HUD/PlayerInventory.gd"
PlayerData="*res://Resources/Player/PlayerData.gd"

[display]

window/size/width=320
window/size/height=180
window/size/test_width=1280
window/size/test_height=720
window/dpi/allow_hidpi=true
window/stretch/mode="2d"
window/stretch/aspect="expand"

[gui]

theme/use_hidpi=true
theme/custom_font="res://Resources/Fonts/pixel_font_8px.tres"

